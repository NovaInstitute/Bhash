@prefix : <https://bhash.dev/hedera/imports/prov/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://bhash.dev/hedera/imports/prov>
    a owl:Ontology ;
    dcterms:title "PROV-O profile for Bhash"@en ;
    dcterms:description "Trimmed subset of PROV-O terms referenced by the Bhash core and consensus modules."@en ;
    dcterms:source <https://www.w3.org/TR/2013/REC-prov-o-20130430/> ;
    dcterms:created "2024-09-20"^^xsd:date ;
    dcterms:modified "2024-09-20"^^xsd:date ;
    owl:imports <http://www.w3.org/ns/prov-o-20130430> ;
    .

prov:Entity a owl:Class ;
    rdfs:label "Entity"@en ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov#> ;
    .

prov:Activity a owl:Class ;
    rdfs:label "Activity"@en ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov#> ;
    .

prov:Agent a owl:Class ;
    rdfs:label "Agent"@en ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov#> ;
    .

prov:Role a owl:Class ;
    rdfs:label "Role"@en ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov#> ;
    .

prov:wasAssociatedWith a owl:ObjectProperty ;
    rdfs:label "was associated with"@en ;
    rdfs:domain prov:Activity ;
    rdfs:range prov:Agent ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov#> ;
    .

prov:wasGeneratedBy a owl:ObjectProperty ;
    rdfs:label "was generated by"@en ;
    rdfs:domain prov:Entity ;
    rdfs:range prov:Activity ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov#> ;
    .

prov:used a owl:ObjectProperty ;
    rdfs:label "used"@en ;
    rdfs:domain prov:Activity ;
    rdfs:range prov:Entity ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov#> ;
    .
