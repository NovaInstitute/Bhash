@prefix : <https://bhash.dev/hedera/alignment/aiao/> .
@prefix hedera: <https://bhash.dev/hedera/core/> .
@prefix aiao: <https://datadudes.xyz/ontology/aiao#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://bhash.dev/hedera/alignment/aiao>
    a owl:Ontology ;
    dcterms:title "AIAO alignment module"@en ;
    dcterms:description "Bridges Hedera consensus and token events to Anthropogenic Impact Accounting Ontology assertions for ESG reporting."@en ;
    dcterms:creator "Bhash Ontology Working Group"@en ;
    dcterms:created "2025-10-01"^^xsd:date ;
    dcterms:modified "2025-10-01"^^xsd:date ;
    owl:imports <https://bhash.dev/hedera/core> ,
        <https://bhash.dev/hedera/consensus> ,
        <https://bhash.dev/hedera/token> ,
        <https://bhash.dev/hedera/mirror-analytics> ;
    owl:versionInfo "0.1.0-dev" ;
    skos:editorialNote "Phase 4 bridge module initiating ESG integration by tying consensus topic evidence and token reserve events to AIAO impact assertions."@en ;
    .

###
# Alignment classes
###

hedera:ConsensusImpactAssertion
    a owl:Class ;
    rdfs:label "Consensus impact assertion"@en ;
    rdfs:subClassOf hedera:TopicMessage ,
        aiao:ImpactAssertion ;
    skos:definition "Consensus Service message curated to substantiate an ESG impact assertion."@en ;
    hedera:sourceDocument <https://docs.hedera.com/hedera/core-concepts/hedera-consensus-service/message-flows> ;
    dcterms:source <https://datadudes.xyz/aiao#ImpactAssertion> ;
    .

hedera:StablecoinReserveAssertion
    a owl:Class ;
    rdfs:label "Stablecoin reserve assertion"@en ;
    rdfs:subClassOf hedera:TokenEvent ,
        aiao:ImpactAssertion ;
    skos:definition "Token event demonstrating reserve movements that underpin sustainability-linked commitments."@en ;
    hedera:sourceDocument <https://hips.hedera.com/hip/hip-540> ;
    dcterms:source <https://datadudes.xyz/aiao#ImpactAssertion> ;
    .

hedera:MirrorImpactIndicator
    a owl:Class ;
    rdfs:label "Mirror impact indicator"@en ;
    rdfs:subClassOf hedera:TelemetryMetric ,
        aiao:ImpactIndicator ;
    skos:definition "Telemetry metric derived from mirror analytics that feeds ESG indicator dashboards."@en ;
    hedera:sourceDocument <https://docs.hedera.com/hedera/mirror-node/architecture/metrics> ;
    dcterms:source <https://datadudes.xyz/aiao#ImpactIndicator> ;
    .

###
# Alignment properties
###

hedera:hasImpactSubject
    a owl:ObjectProperty ;
    rdfs:label "has impact subject"@en ;
    rdfs:domain aiao:ImpactAssertion ;
    rdfs:range hedera:Actor ;
    rdfs:subPropertyOf aiao:hasSubject ;
    skos:definition "Identifies the Hedera actor that an AIAO impact assertion evaluates."@en ;
    hedera:sourceDocument <https://docs.hedera.com/hedera/core-concepts/governing-the-network> ;
    dcterms:source <https://datadudes.xyz/aiao#hasSubject> ;
    .

hedera:reportsImpactIndicator
    a owl:ObjectProperty ;
    rdfs:label "reports impact indicator"@en ;
    rdfs:domain aiao:ImpactAssertion ;
    rdfs:range aiao:ImpactIndicator ;
    rdfs:subPropertyOf aiao:reportsOnIndicator ;
    skos:definition "Links an impact assertion to the indicator captured via Hedera telemetry."@en ;
    hedera:sourceDocument <https://docs.hedera.com/hedera/mirror-node/architecture/metrics> ;
    dcterms:source <https://datadudes.xyz/aiao#reportsOnIndicator> ;
    .

hedera:deliveredBySchedule
    a owl:ObjectProperty ;
    rdfs:label "delivered by schedule"@en ;
    rdfs:domain aiao:ImpactAssertion ;
    rdfs:range hedera:ScheduledTransaction ;
    rdfs:subPropertyOf prov:wasDerivedFrom ;
    skos:definition "Associates an impact assertion with the scheduled transaction that operationalised it."@en ;
    hedera:sourceDocument <https://docs.hedera.com/hedera/core-concepts/scheduled-transactions/how-scheduled-transactions-work> ;
    dcterms:source <https://datadudes.xyz/aiao#ImpactAssertion> ;
    .

hedera:referencesMirrorDataset
    a owl:ObjectProperty ;
    rdfs:label "references mirror dataset"@en ;
    rdfs:domain aiao:ImpactAssertion ;
    rdfs:range hedera:MirrorDataset ;
    rdfs:subPropertyOf aiao:hasEvidenceSource ;
    skos:definition "Connects an impact assertion to the mirror dataset furnishing evidentiary support."@en ;
    hedera:sourceDocument <https://docs.hedera.com/hedera/mirror-node/sdks-and-apis/rest-api> ;
    dcterms:source <https://datadudes.xyz/aiao#hasEvidenceSource> ;
    .

hedera:capturesImpactWindow
    a owl:DatatypeProperty ;
    rdfs:label "captures impact window"@en ;
    rdfs:domain aiao:ImpactAssertion ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf aiao:hasTemporalScope ;
    skos:definition "Specifies the reporting window for an impact assertion expressed with Hedera data."@en ;
    hedera:sourceDocument <https://docs.hedera.com/hedera/core-concepts/transactions/transaction-lifecycle> ;
    dcterms:source <https://datadudes.xyz/aiao#hasTemporalScope> ;
    .
